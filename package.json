{
    "name": "auto-web-test-tools",
    "version": "1.1.0",
    "description": "网页自动测试工具",
    "main": "main.js",
    "scripts": {
        "start": "electron .",
        "package": "electron-packager . auto-web-test-tools --win --out build --arch=x64 --version1.0.0 --overwrite --icon=static/images/128.ico",
        "dist": "electron-builder --win --x64",
        "win32": "electron-builder --win --ia32"
    },
    "repository": "https://github.com/electron/electron-quick-start",
    "keywords": [
        "Electron",
        "quick",
        "start",
        "tutorial",
        "demo"
    ],
    "author": "com.chengfei.tools",
    "license": "CC0-1.0",
    "devDependencies": {
        "electron": "^7.2.4"
    },
    "dependencies": {
        "electron-json-storage": "^4.1.6",
        "regedit": "^3.0.2"
    },
    "build": {
        "appId": "com.phil.tools",
        "copyright": "https://github.com/phil-cheng/auto-web-test-tools.git",
        "productName": "网页自动测试工具",
        "mac": {
            "target": [
                "dmg",
                "zip"
            ]
        },
        "win": {
            "target": [
                "nsis",
                "zip"
            ],
            "icon": "static/images/favicon.ico"
        }
    }
}
